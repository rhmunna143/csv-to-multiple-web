{
  "name": "csv-website-generator",
  "version": "1.0.0",
  "description": "A Node.js-based build tool that reads data from a CSV file and generates multiple standalone React websites",
  "main": "src/generator/index.js",
  "scripts": {
    "start": "node src/generator/index.js",
    "dev": "nodemon src/generator/index.js",
    "clean": "node -e \"require('fs-extra').remove('./build')\"",
    "validate": "node src/generator/csvValidator.js",
    "build:single": "node src/generator/singleBuilder.js"
  },
  "keywords": ["csv", "react", "website-generator", "build-tool"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "papaparse": "^5.4.1",
    "fs-extra": "^11.1.1",
    "cross-spawn": "^7.0.3",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}